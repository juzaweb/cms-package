{
    "name": "juzaweb/cms",
    "description": "Juzaweb CMS is a Content Management System (CMS) developed based on Laravel Framework and web platform whose sole purpose is to make your development workflow simple again. Project develop by Juzaweb",
    "keywords": [
        "juzaweb cms",
        "juzaweb.com cms",
        "juzaweb.com",
        "laravel cms",
        "cms like wordpress",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "intervention/image": "^2.5",
        "juzaweb/config": "^2.2",
        "juzaweb/image-optimizer": "^1.4",
        "juzaweb/laravel-chunk-upload": "^1.4",
        "juzaweb/log-viewer": "^5.2|^8.1",
        "juzaweb/jwt-auth": "^1.0",
        "juzaweb/l5-swagger": "^6.0|^8.0"
    },
    "require-dev": {
        "juzaweb/laravel-cms": "^1.0",
        "phpunit/phpunit": "6.0 | 7.0 | 7.5 | 8.4 | ^8.5"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Tests\\": "tests/",
            "Juzaweb\\": "src/",
            "Juzaweb\\Database\\Factories\\": "database/factories/",
            "Juzaweb\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ],
        "classmap": [
            "database/seeders"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juzaweb\\Providers\\CmsServiceProvider"
            ]
        }
    }
}
